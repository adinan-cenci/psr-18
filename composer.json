{
    "name": "adinan-cenci/psr-18",
    "description": "An implementation of the PSR-18 specification.", 
    "type": "library", 
    "keywords": ["http", "psr", "psr-18"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AdinanCenci\\Psr18\\": "src/",
            "AdinanCenci\\Psr18\\Tests\\": "tests/"
        }
    },
    "require": {
        "adinan-cenci/psr-7": "^1.1.3",
        "adinan-cenci/psr-17": "^1.0",
        "psr/http-client": "^1.0"
    },
    "scripts": {
        "unit-tests": [
            "./vendor/bin/phpunit tests"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    }
}
